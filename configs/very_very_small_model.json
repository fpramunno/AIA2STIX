{
    "model": {
			"type": "vit",
			"in_channels": 2,                
			"input_size": [24, 2],           
			"patch_size": [4, 2],            
			"mapping_out": 64,                 
			"out_channels": 1,           
			"t_out": 5,			
			"depths": [1, 1, 1],                
			"channels": [12, 24, 36],        
			"self_attn_depths": [false, false, false],  
			"has_variance": false,             
			"loss_config": "karras",
			"masked": false,
			"emb_features": 64,
			"loss_weighting": "soft-min-snr",  
			"dropout_rate": 0.05,              
			"augment_wrapper": false,          
			"augment_prob": 0.12,             
			"sigma_data": 0.5,                
			"sigma_min": 0.01,                
			"sigma_max": 100.0,               
			"sigma_sample_density": {
				"type": "cosine-interpolated"},
			
			"hid_channels": 4,               
			"hid_blocks": 1,                  
			"ffn_factor": 2,                   
			"attention_heads": 1,              
			"qk_norm": true,                   
			"rope": true,                      
			"window_size": null,               
			"spatial": 2,                
			"cond_channels": 4              
    
    
    },
    "optimizer": {
        "type": "adamw",
        "lr": 1e-4,
        "betas": [0.95, 0.999],
        "eps": 1e-6,
        "weight_decay": 1e-3
    },
    "lr_sched": {
        "type": "constant",
        "warmup": 0.0
    },
    "ema_sched": {
        "type": "inverse",
        "power": 0.6667,
        "max_value": 0.9999
    }
}